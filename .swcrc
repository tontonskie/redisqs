{
  "$schema": "https://swc.rs/schema.json",
  "sourceMaps": "inline",
  "module": {
    "type": "commonjs",
    "strictMode": true,
    "noInterop": false
  },
  "jsc": {
    "target": "es2023",
    "parser": {
      "syntax": "typescript",
      "decorators": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "paths": {
      "@/testing/*": [
        "./testing/*"
      ],
      "@/*": [
        "./src/*"
      ]
    },
    "baseUrl": "./"
  }
}
