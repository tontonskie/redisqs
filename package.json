{
  "name": "redisqs",
  "version": "0.0.1",
  "description": "Background job processing for Redis",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint && prettier --check .",
    "type-check": "tsc --noEmit --incremental false",
    "format": "prettier --write .",
    "test": "npm run lint && npm run type-check && jest",
    "build": "rm -rf dist && swc src -d dist --strip-leading-paths"
