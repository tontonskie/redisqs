  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tontonskie/redisqs/issues"
  },
  "homepage": "https://github.com/tontonskie/redisqs#readme",
  "dependencies": {
    "redis": "5.8.2"
  },
  "devDependencies": {
    "@eslint/js": "9.35.0",
    "@swc/cli": "0.7.8",
    "@swc/core": "1.13.5",
    "@swc/jest": "0.2.39",
    "@types/jest": "30.0.0",
    "@types/node": "22.18.1",
    "eslint": "9.35.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-prettier": "5.5.4",
    "jest": "30.1.3",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.43.0"
  }
